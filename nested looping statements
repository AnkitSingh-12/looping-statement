#while inside while loop 
# both inner loop and outer loop running completly
i=1
while i<6:
    j=1
    while j<6:
        print(i,j)
        j+=1
    i+=1

#breaking of inner loop with inner loop value
i=1
while i<6:
    j=1
    while j<6:
        print(i,j)
        if j==3:
            break
        j+=1
    i+=1

#breaking of inner loop with outer loop value
i=1
while i<6:
    j=1
    while j<6:
        print(i,j)
        if i==3:
            break
        j+=1
    i+=1


