
#wap to check prime or not 

#for else 


n=int(input())
if n>1:
   for i in range(2,n//2+1):
      if n%i==0:
         print('n is not prime')
         break
   else:
      print('it  is a prime')
else:
   print(n,'is not prime')


# or while else 
    
n=int(input())
i=2
if n>1:
   while i < n//2+1:
      if n%i==0:
         print('n is not prime')
         break
      i+=1
   else:
      print('it  is a prime')
else:
   print(n,'is not prime')
