#2#
#  wap to print a palindrome no 1  to 10 palindrome  no using function 

def ispalindrome(n):
    dummy=n
    rev=0
    while dummy>0:
        rem=dummy%10
        dummy=dummy//10
        rev=rev*10+rem
    if n==rev:
        return True
    else:
        return False
def first_nthpalindromeNo(target):
    n=1
    c=0
    while c<target:
        if ispalindrome(n):
            print(n)
            c+=1
        n+=1
target=int(input())
first_nthpalindromeNo(target)


#2#
#  wap to print a palindrome no 2th palindrome  no using function 

def ispalindrome(n):
    dummy=n
    rev=0
    while dummy>0:
        rem=dummy%10
        dummy=dummy//10
        rev=rev*10+rem
    if n==rev:
        return True
    else:
        return False
def nthpalindromeNo(target):
    n=1
    c=0
    while c<target:
        if ispalindrome(n):
            c+=1
            if c==target:
                print(n,end=' ')
        n+=1
target=int(input())
nthpalindromeNo(target)



#2#
#  wap to print a palindrome no 6 to10 palindrome  no using function 


def ispalindrome(n):
    dummy=n
    rev=0
    while dummy>0:
        rem=dummy%10
        dummy=dummy//10
        rev=rev*10+rem
    if n==rev:
        return True
    else:
        return False
def palindrome_from_6th(n):
    num = 1
    c = 0  
    while c < 10: 
        if ispalindrome(num):
            c += 1
            if c >= 6:  
                print(num)
        num += 1  
palindrome_from_6th(10)


