#1#
#  wap to print a prime no 1  to 10 prime no using function 

def is_prime(n):
    if n>1:
        for i in range(2,n//2+1):
            if n%i==0:
                return False
        else:
                return True
    else:
            return False 
def first_10thprimeNo(target):
    n=2
    c=0
    while c<target:
        if is_prime(n):
            print(n,end=' ')
            c+=1
        n+=1
target=int(input())
first_10thprimeNo(target)
