    dummy=row
    for sp in range (1,spaces+1):
        print(' ',end=' ')
    for st in range(1,stars+1):
            print(dummy,end=' ')
            if row%2==1:
                dummy+=1
            else:
                dummy-=1
    print()
    stars+=1
    spaces-=1



