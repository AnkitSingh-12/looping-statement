for row in range(1,n+1):
    for col in range (1,row+1):
        print(dummy,end=' ')
        dummy+=1
    print()



#----------------------------------------------------#

#18#

'''
        1 
      2 4 
    3 4 5 
  4 6 8 10 
5 6 7 8 9 

'''

n=5
stars=1
spaces=n-1
for row in range (1,n+1):
    dummy=row
    for sp in range (1,spaces+1):
        print(' ',end=' ')
    for st in range(1,stars+1):
            print(dummy,end=' ')
            if row%2==1:
                dummy+=1
            else:
                dummy+=2
    print()
    stars+=1
    spaces-=1
#-----------------------------------------------------------------------------------#
