#14#

'''
1 2 3 4 5 
1 2 3 4 
1 2 3 
1 2 
1 

'''


n=5
stars=n
spaces=0
for row in range (1,n+1):
    dummy=1
    for st in range(1,stars+1):
            print(dummy,end=' ')
            dummy+=1         
    print()
    stars-=1
    spaces+=1

#-----------------------------------------------------------#
#15#
'''
        1 
      2 1 
    3 2 1 
  4 3 2 1 
5 4 3 2 1 

'''

n=5
stars=1
spaces=n-1
for row in range (1,n+1):
    dummy=row
    for sp in range (1,spaces+1):
        print(' ',end=' ')
    for st in range(1,stars+1):
            print(dummy,end=' ')
            dummy-=1        
    print()
    stars+=1
    spaces-=1
    

#----------------------------------------------------#

#16#


'''
5 4 3 2 1 
  4 3 2 1 
    3 2 1 
      2 1 
        1 

'''
n=5
stars=n
spaces=0
for row in range (1,n+1):
    dummy=stars
    for sp in range (1,spaces+1):
        print(' ',end=' ')
    for st in range(1,stars+1):
            print(dummy,end=' ')
            dummy-=1
    print()
    stars-=1
    spaces+=1
