
#9#wapn to find sum of digit present in even index position in string 
s='HELLO123'
sum=0
for ip in range(0,len(s),2):
    if s[ip].isdigit():
        sum+=int(s[ip])
        print(ip)



#10# wap to print sum of odd index postion of even digit
S=input()
s=0
for ip in range(1,len(S),2):
    if S[ip] in '02468':
        s+=ip
print(s)

#output='123456'


#11#wap to print sum of digit in given string 
S=input()
s=0
for ele in S:
    if ele.isdigit():
        s+=int(ele)
print(s)

#output=hekko123

#12#wap to print find the sum of index postion of given digit 

S=input()
s=0
for ip in range(len(S)):
    if S[ip].isdigit():
        s+=int(S[ip])
print(s)
  

  #output 123456    


